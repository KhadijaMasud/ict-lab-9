<script>

  var miniCartSelectors = {
    addToCart: '[data-add-to-cart]',
    addToCartText: '[data-add-to-cart-text]',
    checkoutBtn: '[data-checkout-btn]'
  };

  var $window = $(window),
    $StickyHeader = $('.site-nav--holder.sticky'),
    $StickyProduct = $('.product_page__wrapper .sticky'),
    $productTop = 60,
    $body = $('body');

  $(document).ready(function() {

    openCart = function () {

      $('.mini-cart').load( '/cart?view=minicart .mini-cart form', function () {
        $("#minicart").data( "mmenu" ).open();
        $(miniCartSelectors.checkoutBtn).removeClass('btn--loading').attr('disabled', false);
      });
    };

    // If product detail page, add class to button
    // Update cart price on header and open cart
    $(document).on('cart.requestStarted', function(event, cart) {
      $(miniCartSelectors.checkoutBtn).addClass('btn--loading');
    });

    $(document).on('cart.requestComplete', function(event, cart) {
      $('.header__cart-total').html(slate.Currency.formatMoney(cart.total_price, theme.moneyFormat));
      $(miniCartSelectors.addToCart).removeClass('btn--loading').attr('disabled', false);
      openCart();
      var magnificPopup = $.magnificPopup.instance;
      magnificPopup.close();

      if (cart.item_count > 0) {
        $('.cart-content .hide').each(function () {
          $(this).removeClass('hide');

        });
      } else {
        $('.cart-content .open-cart').removeClass('hide');
        $('.cart-content .checkout').addClass('hide');
      }

      if ($window.scrollTop() > 0) {
        //$StickyHeader.css({'top': $window.scrollTop()}).addClass('static');
        //$StickyProduct.css({'top': $window.scrollTop()+$productTop}).addClass('static');
      }
    });

    // General AJAX success handler (add, remove, change qty)
    $(document).ajaxSuccess(function(event, jqxhr, settings) {
      if(settings.url == "/cart/change.js") {
      }if ($('#minicart').find('[data-item-limiter]').length > 0) {
        $('[data-item-limiter]').each(function(){
          if ($(this).data('item-limiter') === $('[data-product]').data('product')) {
            if ($(this).find('.quantity').val() >= $('[data-limiter]').data('limiter')) {
              $('button[data-add-to-cart]').prop('disabled', true).find('span').html(theme.strings.limitReached);
            }
          } else {
            $('button[data-add-to-cart]').find('span').html(theme.strings.addToCart);
          }
        });
      } else {
        $('button[data-add-to-cart]').find('span').html(theme.strings.addToCart);
      }
    });

    // Handle errors when adding to cart
    $(document).ajaxError(function(event, jqxhr, settings, thrownError) {
      if(settings.url == "/cart/add.js") {
        var response = JSON.parse(jqxhr.responseText);

        var message = response.description;

        // Trying to add more into the cart than available
        if (message.indexOf('You can only add') != -1) {
          var count = message.split('You can only add')[1].split('to the cart')[0].trim();
          var message = 'Sorry, you can only add a further {{ count }} into your cart'.replace('\{\{ count \}\}', count);
        }
        if (message.substring(0, 3) === "All") {
          var product = message.substring(3).split('are in your cart')[0].trim().split(" ");
          product.shift();
          product = product.join(" ");
          var message = 'Sorry, you already have all available {{ product }} in your cart'.replace('\{\{ product \}\}', product);
        }

        // Show modal with error message
        alert (message);

        $(miniCartSelectors.addToCart).prop('disabled', false);
        $(miniCartSelectors.addToCartText).html(theme.strings.addToCart);
      }
    });


    // This button will increment the value to the inventory max
    $body.on('click', '.qtyplus', function(e){
      e.preventDefault();

      var $holder = $(this).closest('.quantity-holder');
      var $input = $holder.find('input');

      var currentVal = parseInt($input.val());
      if (!isNaN(currentVal) && currentVal < $input.attr('max')) {
        $input.val(currentVal + 1).trigger('change');
      } else {
        $input.val($input.attr('max')).trigger('change');
      }
    });

    // This button will decrement the value till 1
    $body.on('click', '.qtyminus',function(e) {
      e.preventDefault();
      var $holder = $(this).closest('.quantity-holder');
      var $input = $holder.find('input');
      var currentVal = parseInt($input.val());
      if (!isNaN(currentVal) && currentVal > 1) {
        $input.val(currentVal - 1).trigger('change');
      } else {
        $input.val(1).trigger('change');
      }
    });


    $(document).on('click', '#minicart .remove-item', function () {
      var $id = $(this).closest('.mini-cart__item').data('item');
      $(this).closest('.mini-cart__item').addClass('remove');
      $(miniCartSelectors.checkoutBtn).addClass('btn--loading');
      CartJS.removeItemById($id);
    });


    $(document).on('change', '.mini-cart__qty .quantity', function() {
      var $this = $(this);
      CartJS.updateItem($this.attr('data-update-quantity'), $this.val());
      $(miniCartSelectors.checkoutBtn).addClass('btn--loading');
    });

  });

</script>





