      <script>
  $(document).ready(function() {
    var $menuNavItem =  $('header .site-nav li.contain-mega a');
    var $menuHolder = $('.mega-menu');
    var $menuNavHolder = $('.mega-menu__navigation-holder');
    var initialTopNav = 0,
      max_size= 750,
      $window = $(window),
      $header = $('header.main-header');

    var pointerPosition = function (item) {
      if (!$menuHolder.hasClass('is-active')){
        $menuNavHolder.css({'opacity':0, 'z-index': 1});
        $(item).addClass('is-open');
        var menu = '.menu-'+($(item).parent().index() + 1);
        if ($window.width() < max_size) {
          $menuHolder.css({top: $('.site-nav--inner').outerHeight() + 33, height : $(menu).outerHeight() - 5 });
        } else {
          $menuHolder.css({top: $('.site-nav--inner').outerHeight(), height : $(menu).outerHeight() - 5 });
        }
        $(menu).css({'opacity':1, 'z-index': 5});
      }
    };

    if ($('html').hasClass('touchevents')) {
      $menuNavItem.on('click', function(e){
        event.preventDefault();
        if ( $(this).parent().hasClass('contain-mega')) {
          if ($(this).hasClass('is-open')) {
            $(this).removeClass('is-open');
            $menuHolder.removeClass('is-active');
          } else {
            if ($menuHolder.hasClass('is-active')) {
              $menuHolder.removeClass('is-active');
              $menuNavItem.removeClass('is-open');
            }
            pointerPosition(e.target);
            $menuHolder.addClass('is-active')
          }
        }

      });
    } else {
      $menuNavItem.on('mouseover', function(e){
        openMegamenu(e)

      }).on('mouseleave', function () {
        clearTimeout(openDelayed);
      });
    }

    var openDelayed;

    function openMegamenu(e) {
      clearTimeout(openDelayed);
      openDelayed = setTimeout(function(){
        if ( $(e.target).parent().hasClass('contain-mega')) {
          if ($menuHolder.hasClass('is-active')) {
            $menuHolder.removeClass('is-active');
            $menuNavItem.removeClass('is-open');
          }
          pointerPosition(e.target);
          $menuHolder.addClass('is-active')
        } else {
          $menuHolder.removeClass('is-active');
          $menuNavItem.removeClass('is-open');
        }
      }, 250);
    }


    $menuHolder.on('mouseleave', function(){
      $(this).removeClass('is-active');
      $menuNavItem.removeClass('is-open');
    });

    function resize() {
      $menuHolder.removeClass('is-active');
      $menuNavItem.removeClass('is-open');
    }
    $( window ).resize(resize).trigger('resize');

    // Sticky class
    var $nav = $('#shopify-section-header');
    var $logoHeader = $('#shopify-section-header-messages');

    $logoHeader.on('scrollSpy:enter', function() {
      $nav.removeClass('fixed');

    });
    $logoHeader.on('scrollSpy:exit', function() {
      console.log('exit:', $(this).attr('id'));
      $nav.addClass('fixed');
    });

    if ($nav.visible(true)) {
      $nav.addClass('fixed');
    }

    $.scrollSpy($logoHeader);

  });
</script>
